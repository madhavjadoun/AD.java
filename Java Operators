import java.util.Scanner;
public class arry{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int m=sc.nextInt();
        int f=sc.nextInt();
     System.out.println("ADDITION "+(m+f));
    System.out.println("SUBTRACTION "+(m-f));
    System.out.println("MULTIPLICATION "+(m*f));
    System.out.print("DIVISION "+(m/f));
    }
}
